{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "project": ["tsconfig.json"],
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/no-extraneous-class": 0,
        "n/no-callback-literal": 0,
        "@typescript-eslint/restrict-template-expressions": 0
    }
}
